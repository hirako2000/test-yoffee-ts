{
  "name": "test-yoffee-ts",
  "version": "0.0.1",
  "description": "some test of Yoffee wiht TypeScript",
  "scripts": {
    "build": "cross-env NODE_ENV=production env=production parcel build src/index.html",
    "start": "cross-env NODE_ENV=dev parcel serve src/index.html",
    "dev": "npm run start",
    "lint": "lynt --typescript",
    "lint-fix": "lynt --typescript --fix",
    "test": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hirako2000/test-yoffee-ts.git"
  },
  "keywords": [
    "yoffee",
    "typescript"
  ],
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@parcel/packager-raw-url": "^2.0.0-rc.0",
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@parcel/transformer-sass": "^2.3.1",
    "@parcel/transformer-webmanifest": "^2.0.0-rc.0",
    "autoprefixer": "^10.3.6",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.8",
    "lynt": "^0.5.5",
    "parcel": "^2.0.0-rc.0",
    "postcss": "^8.3.8",
    "postcss-fontpath": "^1.0.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "daisyui": "^1.25.4",
    "tailwindcss": "^3.0.23",
    "yoffee": "0.0.9"
  },
  "transformers": {
    "*.ts": [
      "@parcel/transformer-typescript"
    ]
  }
}
